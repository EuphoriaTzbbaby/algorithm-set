class Solution:
    def countWays(self, nums: List[int]) -> int:
        nums.sort()
        n = len(nums)
        """
        0 2 3 3 6 6 7 7 
        
        """
        ans = 1 if nums[0]  else 0
        for i in range(n):
            if i + 1 < n and nums[i] < i + 1
                ans += 1
        return ans + 1
