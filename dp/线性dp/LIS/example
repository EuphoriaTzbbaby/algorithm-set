******************************************************************************leetcode**************************************************************************************
300 
class Solution:
    def lengthOfLIS(self, nums: List[int]) -> int:
        n = len(nums)
        # f[i] 表示以 nums[i] 结尾最长上升子序列的长度
        f = [1] * n 
        for i in range(n):
            for j in range(i):
                if nums[i] > nums[j]:
                    f[i] = max(f[i], f[j] + 1)
        return max(f)
