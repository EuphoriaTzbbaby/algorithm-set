*********************************************************************************leetcode*********************************************************************************

646.最长数对链
class Solution:
    def findLongestChain(self, pairs: List[List[int]]) -> int:
        pairs.sort()
        n = len(pairs)
        f = [1] * n 
        for i in range(n):
            for j in range(n):
                if pairs[j][1] < pairs[i][0]:
                    f[i] = max(f[i], f[j] + 1)
        return max(f)
